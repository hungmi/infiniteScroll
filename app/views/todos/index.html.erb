<p id="notice"><%= notice %></p>

<h1>Listing Todos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="todos">
    <%= render @todos %>
  </tbody>
</table>

<br>

<%= link_to 'New Todo', new_todo_path %>
<div class='paginator'>
  <%= paginate @todos, remote: true %>
</div>